{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "install:example": "pnpm add ** --filter **/** packageName 包名",
    "global:example": "pnpm add ** -W || --workspace-root",
    "dev:global:example": "pnpm add ** -w -D",
    "preview": "vite preview",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prettier": "prettier --write .",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "eslint": "eslint . --ext .ts,.vue,.js,.tsx",
    "preinstall": "node ./scripts/prelint/preinstall.js"
  },
  "dependencies": {
    "vue": "^3.2.25",
    "dayjs": "^1.10.7"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "eslint": "^8.9.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.3",
    "turbo": "latest",
    "vite": "^2.8.6",
    "vue-tsc": "^0.29.8",
    "rollup": "^2.67.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.3.1",
    "babel-jest": "^27.4.5"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.28.0"
}
